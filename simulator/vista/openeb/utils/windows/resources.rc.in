PRODUCTVERSION    @PROJECT_VERSION_MAJOR@,@PROJECT_VERSION_MINOR@,@PROJECT_VERSION_PATCH@,0
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904B0"
        BEGIN
            VALUE "CompanyName",        "Prophesee"
            VALUE "FileDescription",    ""
            VALUE "FileVersion",        "@PROJECT_VERSION_FULL@"
            VALUE "InternalName",       ""
