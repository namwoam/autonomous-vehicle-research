            VALUE "OriginalFilename",   "@dll_filename@"
            VALUE "ProductName",        "Metavision SDK"
            VALUE "ProductVersion",     "@PROJECT_VERSION_FULL@"
        END
    END

    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1252
    END
END